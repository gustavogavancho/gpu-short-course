    "# Task 1.5\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "x = np.linspace(-1, 1, 20)\n",
    "\n",
    "# Task 1.6\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def f(x):\n",
    "    return x*x\n",
    "\n",
    "plt.plot(x, f(x))\n",
    "\n",
    "def g(x):\n",
    "    return 2*x\n",
    "\n",
    "plt.plot(x, g(x))\n",
    "plt.grid(\"on\")"
   ]
